<h1>profile</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
 <%= form_for(:edit, url: edit_settings_path) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :country %>
      <%= f.text_field :country, class: 'form-control' %>

      <%= f.submit "update", class: "btn btn-primary" %>
    <% end %>
 </div>
</div>